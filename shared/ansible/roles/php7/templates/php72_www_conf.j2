[default]
listen = {{ php72_socket_dir }}{{ php72_socket_file }}
listen.owner = {{  php72_nginx_user }}
listen.group = {{  php72_nginx_user }}
listen.mode = 0660
user = {{  php72_nginx_user }}
group = {{  php72_nginx_user }}
pm = dynamic
pm.max_children = 75
pm.start_servers = 8
pm.min_spare_servers = 5
pm.max_spare_servers = 20
pm.max_requests = 500
